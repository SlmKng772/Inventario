@model IEnumerable<MVCInvTI.Models.InvMaq>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.idProd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodMaq)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observacoes)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.idProd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodMaq)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observacoes)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.idMaq">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.idMaq">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.idMaq">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
